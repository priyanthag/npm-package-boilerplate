{
  "name": "npm-boilerplate",
  "version": "0.0.1",
  "keywords": [
    "NPM Package boilerplate"
  ],
  "author": "Priyantha Gunawardena <pgdesigning@gmail.com>",
  "license": "MIT",
  "description": "<npm-package>",
  "homepage": "https://github.com/priyanthag/<npm-package>#readme",
  "url": "https://github.com/priyanthag/<npm-package>/issues",
  "email": "pgdesigning@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/priyanthag/<npm-package>.git"
  },
  "main": "bin/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "npm run lint",
    "clean": "rm -rf ./bin",
    "build": "npm run clean && babel src --out-dir bin",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "test": "cross-env NODE_ENV=test TZ=Etc/UTC jest",
    "test:single": "npm run test",
    "test:impacted": "npm run test -- --onlyChanged",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage:view": "http-server ./coverage/lcov-report/"
  },
  "pre-commit": [
    "check",
    "test:impacted"
  ],
  "dependencies": {
    "aws-sdk": "^2.418.0",
    "lodash-es": "^4.17.15",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-destructuring": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.8.4",
    "@types/jest": "^25.1.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "http-server": "^0.12.1",
    "jest": "^25.1.0",
    "pre-commit": "^1.2.2"
  }
}
